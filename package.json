{
  "name": "stress-test",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "compile": "hardhat compile",
    "clean": "npx hardhat clean",
    "deploy:mumbai": "npx hardhat --network mumbai deploy",
    "gas": "cross-env REPORT_GAS=true hardhat test",
    "coverage": "cross-env HARDHAT_DEPLOY_FIXTURE=true hardhat coverage"
  },
  "author": "Gulam Rasul Shah",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.10",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@truffle/hdwallet-provider": "^2.0.15",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.0.2",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.0.2",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "chai": "^4.3.7",
    "chai-ethers": "^0.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.2.0",
    "ethers": "^5.3.1",
    "hardhat": "^2.3.3",
    "hardhat-deploy": "^0.8.7",
    "hardhat-gas-reporter": "^1.0.9",
    "lint-staged": "^13.0.4",
    "mocha": "^9.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-solidity": "^1.0.0",
    "prompt-sync": "^4.2.0",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.4",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.0",
    "cross-env": "^7.0.3"
  },
  "lint-staged": {
    "*.{ts,json,sol,md,yml}": "prettier --write"
  }
}
